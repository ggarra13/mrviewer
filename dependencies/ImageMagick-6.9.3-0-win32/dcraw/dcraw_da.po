msgstr "Ejer: %s\n"

msgid "DNG Version: "
msgstr "DNG-version: "

msgid "ISO speed: %d\n"
msgstr "ISO-værdi: %d\n"

msgid "Shutter: "
msgstr "Lukkertid: "

