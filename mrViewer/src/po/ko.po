msgid "White"
msgstr "하얀색"

#: E:/code/applications/mrv/BUILD/Windows-6.3-64/Release/tmp/mrViewer/src/mrvPreferencesUI.cxx:2674
msgid "White Luminance"
msgstr "백색 휘도"

